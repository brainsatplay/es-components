{
    "__children": {
        "counter": {
            "__compose": "./components/counter.js",
            "__animate": true
        },
        "timestamp": {
            "__compose": "./components/timestamp.js",
            "__animate": true
        },
        "count": {
            "__element": "p",
            "__children": {
                "header": {
                    "__element": "b",
                    "__attributes": {
                        "innerText": "Frames: "
                    }
                },
                "span": {
                    "__element": "span"
                }
            }
        },
        "time": {
            "__element": "p",
            "__children": {
                "header": {
                    "__element": "b",
                    "__attributes": {
                        "innerText": "Time: "
                    }
                },
                "span": {
                    "__element": "span"
                }
            }
        },
        }
    },
    "__listeners": {
        "count.span": {
            "counter": true
        },
        "time.span": {
            "timestamp": true
        }
    }
}