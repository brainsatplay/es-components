{
    "tagName": "div",
    "esComponents": {
        "connectmode": {
            "esCompose": "../select.js"
        },
        "selectUSB": {
            "esCompose": "../select.js"
        },
        "selectBLE": {
            "esCompose": "../select.js"
        },
        "selectBLEOther": {
            "esCompose": "../select.js"
        },
        "selectOther": {
            "esCompose": "../select.js"
        },
        "connect": {
            "esCompose": "../../devices/connect.js"
        },
        "toConnect": {
            "attributes": {
                "innerHTML": "Connect Device"
            },
            "esCompose": "../button.js"
        }
    },
    "listeners": {
        "selectBLE": {
            "toConnect": true
        },
        "selectUSB": {
            "toConnect": true
        },
        "selectBLEOther": {
            "toConnect": true
        },
        "selectOther": {
            "toConnect": true
        },
        "connectmode": {
            "connect.mode": true
        },
        "toConnect": {
            "connect": true
        }
    }
}