{
    "esComponents": {
        "list": {
            "esCompose": "../../../components/ui/ul.js"
        },
        "form": {
            "tagName": "form",
            "attributes": {
                "onsubmit": {
                    "esCompose": "./scripts/onSubmit.js"
                }
            },
            "esComponents": {}
        },
        "input": {
            "esCompose": "../../../components/ui/input.js"
        },
        "button": {
            "attributes": {
                "innerHTML": "Add Todo",
                "type": "submit"
            },
            "esCompose": "../../../components/ui/button.js"
        },
        "store": {
            "esCompose": "../../../components/storage/local/set.js"
        },
        "load": {
            "arguments": {
                "key": "todos"
            },
            "esCompose": "../../../components/storage/local/get.js"
        }
    },
    "listeners": {
        "load": {
            "list": true
        },
        "input": {
            "button": true
        },
        "button": {
            "log": true,
            "list": true
        },
        "list": {
            "store": true
        }
    }
}